<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.spring.backup.datasource.repository.BackupMemberMapper">
    <insert id="insertMember" parameterType="com.example.spring.base.dto.MemberVO">
        insert into member (name, email)
        values (#{name}, #{email})
    </insert>
</mapper>
